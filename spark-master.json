{
    "id": "/sandbox-1/spark-master",
    "cpus": 0.5,
    "mem": 1024,
    "instances": 1,
    "cmd": "start-spark master",
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "singularities/spark:1.6",
            "forcePullImage": true,
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 4040,
                    "hostPort": 0,
                    "servicePort": 0,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 8080,
                    "hostPort": 0,
                    "servicePort": 0,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 8081,
                    "hostPort": 0,
                    "servicePort": 0,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 7077,
                    "hostPort": 0,
                    "servicePort": 10077,
                    "protocol": "tcp"
                }
            ]
        }
    }
}